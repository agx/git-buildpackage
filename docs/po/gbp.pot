"<refentrytitle>gitattributes</refentrytitle> <manvolnum>5</manvolnum> "
"</citerefentry>, <citerefentry> <refentrytitle>dgit</refentrytitle> "
"<manvolnum>7</manvolnum> </citerefentry>"
msgstr ""

